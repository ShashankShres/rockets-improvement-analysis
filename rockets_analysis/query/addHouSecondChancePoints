SET SCP = 16.7
WHERE Season = '2022-2023';

UPDATE rocketsPerGame
SET SCP = 14.2
WHERE Season = '2021-2022';

UPDATE rocketsPerGame
SET SCP = 12.2
WHERE Season = '2020-2021';
